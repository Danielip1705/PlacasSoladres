<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PlacasSoladres.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PlacasSoladres.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="PlacasSoladres">

    <ShellContent
        ContentTemplate="{DataTemplate local:LoginView}"
        Route="LoginView" />
    
    <ShellContent
        ContentTemplate="{DataTemplate local:Citas}"
        Route="Citas" />

    <ShellContent
        ContentTemplate="{DataTemplate local:DetallesCitas}"
        Route="Detalles"/>

</Shell>
